   "source": [
    "from google_play_scraper import reviews\n",
    "import pandas as pd\n",
    "\n",
    "result, _ = reviews(\n",
    "    'com.paynep.moru',\n",
    "    count=8000,\n",
    ")\n",
    "\n",
    "df = pd.DataFrame(result)[['content', 'score', 'at']]\n",
    "df.columns = ['review_text', 'rating', 'date']\n",
    "\n",
    "df.to_csv('moru_reviews_static.csv', index=False)"
   ]
  }
